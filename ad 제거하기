#include <string>
#include <vector>

using namespace std;

vector<string> solution(vector<string> strArr) {
    vector<string> answer;
    
    for(int i = 0; i < strArr.size(); ++i) {
        if(strArr[i].find("ad") == string::npos) answer.push_back(strArr[i]);  // "ad"라는 부분 문자열 포함할 경우 모든 문자열 제거
    }
    return answer;
}
