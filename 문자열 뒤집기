#include <string>
#include <vector>
#include <algorithm>

using namespace std;

string solution(string my_string, int s, int e) {
    string answer = my_string;
    // reverse(str.begin(), str.end()) : string의 처음부터 끝까지 뒤집음
    reverse(answer.begin() + s, answer.begin() + e + 1);
    return answer;
}
