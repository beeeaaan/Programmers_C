#include <iostream>

using namespace std;

int main(void) {
    int number;
    cin >> number;
    
    int answer = 0;
    
    while(number != 0){  // for문을 while문으로 변경 (for(int i=0; i<1; i++))
        answer += number % 100;
        number /= 100;
    }
    cout << answer << endl;
    return 0;
}
