#include <string>
#include <vector>
#include <algorithm>

using namespace std;

// 팩토리얼 계산 함수
long long factorial(int num) {
    if (num == 1) {
        return 1;
    }
    return num * factorial(num - 1);
}

vector<int> solution(int n, long long k) {
    vector<int> answer, availableNum; // 결과를 저장할 벡터와 사용할 수 있는 숫자들
    long long factorialVal, idx;

    for (int i = 1; i <= n; i++) {
        availableNum.push_back(i);
    }
    
    k--;

    while (k != 0) {
        factorialVal = factorial(--n);
        // 현재 숫자의 인덱스 찾기
        idx = k / factorialVal;
        // 다음에 찾을 인덱스 구하기
        k = k % factorialVal;
        // 결정된 숫자 결과에 추가하고, 해당 숫자는 제거
        answer.push_back(availableNum.at(idx));
        availableNum.erase(availableNum.begin() + idx);
    }

    // 남아있는 숫자들 결과에 순서대로 추가
    for (int i = 0; i < availableNum.size(); i++) {
        answer.push_back(availableNum[i]);
    }

    return answer;
}
