#include <bits/stdc++.h>

using namespace std;

int solution(vector<int> number) {
    int answer = 0;
    
    // 삼총사 첫 번째
    for(int i = 0; i < number.size() - 2; i++) {
        // 삼총사 두 번째
        for(int j = i + 1; j < number.size() - 1; j++) {
            // 삼총사 세 번째
            for(int k = j + 1; k < number.size(); k++) {
                // 0이 될 경우 삼총사
                if(number[i] + number[j] + number[k] == 0) answer++;
            }
        }
    }
    
    return answer;
}
