#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

double solution(int numbers[], size_t numbers_len) {  // numbers_len : 배열 numbers 길이
    double answer = 0;
    double sum = 0;
    
    for(int i = 0; i < numbers_len; i++){
        sum = sum + numbers[i];
    }    
    answer = sum / numbers_len;  // 배열 합 / 배열 길이
    
    return answer;
}
